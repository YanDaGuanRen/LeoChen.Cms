<?xml version="1.0" encoding="utf-8"?>
<EntityModel xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:schemaLocation="https://newlifex.com https://newlifex.com/Model202509.xsd" schemaLocation="https://newlifex.com/Model202407.xsd" Document="https://newlifex.com/xcode/model" xmlns="https://newlifex.com/Model202509.xsd">
  <Option>
    <!--类名模板。其中{name}替换为Table.Name，如{name}Model/I{name}Dto等-->
    <ClassNameTemplate />
    <!--显示名模板。其中{displayName}替换为Table.DisplayName-->
    <DisplayNameTemplate />
    <!--基类。可能包含基类和接口，其中{name}替换为Table.Name-->
    <BaseClass>Entity</BaseClass>
    <!--命名空间-->
    <Namespace>LeoChen.Cms.Data</Namespace>
    <!--输出目录-->
    <Output>.\</Output>
    <!--是否使用中文文件名。默认false-->
    <ChineseFileName>False</ChineseFileName>
    <!--用于生成Copy函数的参数类型。例如{name}或I{name}-->
    <ModelNameForCopy />
    <!--带有索引器。实现IModel接口-->
    <HasIModel>False</HasIModel>
    <!--可为null上下文。生成String?等-->
    <Nullable>False</Nullable>
    <!--数据库连接名-->
    <ConnName>Membership</ConnName>
    <!--模型类模版。设置后生成模型类，用于接口数据传输，例如{name}Model-->
    <ModelClass>{name}Model</ModelClass>
    <!--模型类输出目录。默认当前目录的Models子目录-->
    <ModelsOutput>.\Models\</ModelsOutput>
    <!--模型接口模版。设置后生成模型接口，用于约束模型类和实体类，例如I{name}-->
    <ModelInterface>I{name}</ModelInterface>
    <!--模型接口输出目录。默认当前目录的Interfaces子目录-->
    <InterfacesOutput>.\Interfaces\</InterfacesOutput>
    <!--用户实体转为模型类的模型类。例如{name}或{name}DTO-->
    <ModelNameForToModel />
    <!--命名格式。Default/Upper/Lower/Underline-->
    <NameFormat>Default</NameFormat>
    <!--魔方区域显示名-->
    <DisplayName>文章内容</DisplayName>
    <!--魔方控制器输出目录-->
    <CubeOutput>../../LeoChen.Cms/Areas/ArticleContent</CubeOutput>
  </Option>
  <Tables>
    <Table Name="CmsContent" Description="内容管理表">
      <Columns>
        <Column Name="ID" DataType="Int32" Identity="True" PrimaryKey="True" Description="主键ID" />
        <Column Name="TenantId" DataType="Int32" Map="XCode.Membership.Tenant@Id@Name@TenantName" Description="租户" />
        <Column Name="AreaID" DataType="Int32" Map="CmsArea@ID@Name" Description="区域代码" />
        <Column Name="ContentSortID" DataType="Int32" Map="CmsContent_Sort@ID@Name" Description="分类代码" />
        <Column Name="ContentSortSubID" DataType="Int32" Map="CmsContent_Sort@ID@Name" Description="子分类代码" />
        <Column Name="Title" DataType="String" Master="True" Length="100" Nullable="False" Description="标题" />
        <Column Name="Titlecolor" DataType="String" Length="7" Description="标题颜色" />
        <Column Name="Subtitle" DataType="String" Length="100" Description="副标题" />
        <Column Name="Filename" DataType="String" Description="文件名" />
        <Column Name="Author" DataType="String" Length="30" Description="作者" />
        <Column Name="Source" DataType="String" Length="30" Description="来源" />
        <Column Name="Outlink" DataType="String" Length="100" Description="外部链接" />
        <Column Name="Date" DataType="DateTime" Description="日期" />
        <Column Name="Ico" DataType="String" Length="100" Description="图标" />
        <Column Name="Pics" DataType="String" Length="1000" Description="图片集" />
        <Column Name="Content" DataType="String" RawType="LONGTEXT" Length="65535" Description="内容" />
        <Column Name="Tags" DataType="String" Length="500" Description="标签" />
        <Column Name="Enclosure" DataType="String" Length="100" Description="附件" />
        <Column Name="Keywords" DataType="String" Length="200" Description="关键词" />
        <Column Name="Description" DataType="String" Length="500" Description="描述" />
        <Column Name="Sorting" DataType="Int32" Description="排序" />
        <Column Name="Status" DataType="Boolean" Description="状态" />
        <Column Name="IsTop" DataType="Boolean" Description="是否置顶" />
        <Column Name="IsRecommend" DataType="Boolean" Description="是否推荐" />
        <Column Name="IsHeadline" DataType="Boolean" Description="是否头条" />
        <Column Name="Visits" DataType="Int32" Description="访问数" />
        <Column Name="Likes" DataType="Int32" Description="点赞数" />
        <Column Name="Oppose" DataType="Int32" Description="反对数" />
        <Column Name="Picstitle" DataType="String" Length="1000" Description="图片标题" />
        <Column Name="CreateUserID" DataType="Int32" Description="创建者" Category="扩展信息" />
        <Column Name="CreateTime" DataType="DateTime" Description="创建时间" Category="扩展信息" />
        <Column Name="CreateIP" DataType="String" Description="创建地址" Category="扩展信息" />
        <Column Name="UpdateUserID" DataType="Int32" Description="更新者" Category="扩展信息" />
        <Column Name="UpdateTime" DataType="DateTime" Description="更新时间" Category="扩展信息" />
        <Column Name="UpdateIP" DataType="String" Description="更新地址" Category="扩展信息" />
        <Column Name="Remark" DataType="String" Length="200" Description="备注" Category="扩展信息" />
      </Columns>
      <Indexes>
        <Index Columns="AreaID" />
        <Index Columns="TenantId,Title" Unique="True" />
        <Index Columns="TenantId" />
        <Index Columns="Date" />
        <Index Columns="Filename" />
        <Index Columns="ContentSortID" />
        <Index Columns="Sorting" />
        <Index Columns="Status" />
        <Index Columns="ContentSortSubID" />
        <Index Columns="Istop" />
        <Index Columns="IsRecommend" />
        <Index Columns="IsHeadline" />
      </Indexes>
    </Table>
  </Tables>
</EntityModel>