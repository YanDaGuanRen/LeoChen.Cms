<?xml version="1.0" encoding="utf-8"?>
<EntityModel xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:schemaLocation="https://newlifex.com https://newlifex.com/Model202509.xsd" schemaLocation="https://newlifex.com/Model202407.xsd" Document="https://newlifex.com/xcode/model" xmlns="https://newlifex.com/Model202509.xsd">
  <Option>
    <!--类名模板。其中{name}替换为Table.Name，如{name}Model/I{name}Dto等-->
    <ClassNameTemplate />
    <!--显示名模板。其中{displayName}替换为Table.DisplayName-->
    <DisplayNameTemplate />
    <!--基类。可能包含基类和接口，其中{name}替换为Table.Name-->
    <BaseClass>Entity</BaseClass>
    <!--命名空间-->
    <Namespace>LeoChen.Cms.Data</Namespace>
    <!--输出目录-->
    <Output>.\</Output>
    <!--是否使用中文文件名。默认false-->
    <ChineseFileName>False</ChineseFileName>
    <!--用于生成Copy函数的参数类型。例如{name}或I{name}-->
    <ModelNameForCopy />
    <!--带有索引器。实现IModel接口-->
    <HasIModel>False</HasIModel>
    <!--可为null上下文。生成String?等-->
    <Nullable>False</Nullable>
    <!--数据库连接名-->
    <ConnName>Membership</ConnName>
    <!--模型类模版。设置后生成模型类，用于接口数据传输，例如{name}Model-->
    <ModelClass>{name}Model</ModelClass>
    <!--模型类输出目录。默认当前目录的Models子目录-->
    <ModelsOutput>.\Models\</ModelsOutput>
    <!--模型接口模版。设置后生成模型接口，用于约束模型类和实体类，例如I{name}-->
    <ModelInterface>I{name}</ModelInterface>
    <!--模型接口输出目录。默认当前目录的Interfaces子目录-->
    <InterfacesOutput>.\Interfaces\</InterfacesOutput>
    <!--用户实体转为模型类的模型类。例如{name}或{name}DTO-->
    <ModelNameForToModel />
    <!--命名格式。Default/Upper/Lower/Underline-->
    <NameFormat>Default</NameFormat>
    <!--魔方区域显示名-->
    <DisplayName>扩展内容</DisplayName>
    <!--魔方控制器输出目录-->
    <CubeOutput>../../LeoChen.Cms/Areas/ExpandContent</CubeOutput>
  </Option>
  <Tables>
    <Table Name="CmsSlide" Description="轮播图片">
      <Columns>
        <Column Name="ID" DataType="Int32" Identity="True" PrimaryKey="True" Description="主键ID" />
        <Column Name="SlideGroupID" DataType="Int32" Description="组ID" />
        <Column Name="Title" DataType="String" Master="True" Description="标题" />
        <Column Name="Subtitle" DataType="String" Length="100" Description="副标题" />
        <Column Name="Pic" DataType="String" Length="100" Description="图片" />
        <Column Name="Link" DataType="String" Length="100" Description="链接" />
        <Column Name="Sorting" DataType="Int32" Description="排序" />
        <Column Name="CreateUserID" DataType="Int32" Description="创建者" Category="扩展信息" />
        <Column Name="CreateTime" DataType="DateTime" Description="创建时间" Category="扩展信息" />
        <Column Name="CreateIP" DataType="String" Description="创建地址" Category="扩展信息" />
        <Column Name="UpdateUserID" DataType="Int32" Description="更新者" Category="扩展信息" />
        <Column Name="UpdateTime" DataType="DateTime" Description="更新时间" Category="扩展信息" />
        <Column Name="UpdateIP" DataType="String" Description="更新地址" Category="扩展信息" />
      </Columns>
      <Indexes>
        <Index Columns="Sorting" />
        <Index Columns="SlideGroupID" />
      </Indexes>
    </Table>
    <Table Name="CmsLink" Description="友情链接">
      <Columns>
        <Column Name="ID" DataType="Int32" Identity="True" PrimaryKey="True" Description="主键ID" />
        <Column Name="LinkGroupID" DataType="Int32" Description="组ID" />
        <Column Name="Name" DataType="String" Master="True" Description="名称" />
        <Column Name="Link" DataType="String" Length="100" Description="友情链接" />
        <Column Name="Logo" DataType="String" Length="100" Description="图标" />
        <Column Name="Sorting" DataType="Int32" Description="排序" />
        <Column Name="CreateUserID" DataType="Int32" Description="创建者" Category="扩展信息" />
        <Column Name="CreateTime" DataType="DateTime" Description="创建时间" Category="扩展信息" />
        <Column Name="CreateIP" DataType="String" Description="创建地址" Category="扩展信息" />
        <Column Name="UpdateUserID" DataType="Int32" Description="更新者" Category="扩展信息" />
        <Column Name="UpdateTime" DataType="DateTime" Description="更新时间" Category="扩展信息" />
        <Column Name="UpdateIP" DataType="String" Description="更新地址" Category="扩展信息" />
      </Columns>
      <Indexes>
        <Index Columns="Sorting" />
        <Index Columns="LinkGroupID" />
      </Indexes>
    </Table>
    <Table Name="CmsTags" Description="文章内链">
      <Columns>
        <Column Name="ID" DataType="Int32" Identity="True" PrimaryKey="True" Description="主键ID" />
        <Column Name="AreaID" DataType="Int32" Map="CmsArea@ID@Name" Description="区域代码" />
        <Column Name="Name" DataType="String" Master="True" Description="名称" />
        <Column Name="Link" DataType="String" Length="200" Description="链接" />
        <Column Name="CreateUserID" DataType="Int32" Description="创建者" Category="扩展信息" />
        <Column Name="CreateTime" DataType="DateTime" Description="创建时间" Category="扩展信息" />
        <Column Name="CreateIP" DataType="String" Description="创建地址" Category="扩展信息" />
        <Column Name="UpdateUserID" DataType="Int32" Description="更新者" Category="扩展信息" />
        <Column Name="UpdateTime" DataType="DateTime" Description="更新时间" Category="扩展信息" />
        <Column Name="UpdateIP" DataType="String" Description="更新地址" Category="扩展信息" />
      </Columns>
      <Indexes>
        <Index Columns="AreaID" />
        <Index Columns="Name" Unique="True" />
      </Indexes>
    </Table>
    <Table Name="CmsForm" Description="自定义表单">
      <Columns>
        <Column Name="ID" DataType="Int32" Identity="True" PrimaryKey="True" Description="编号" />
        <Column Name="Name" DataType="String" Master="True" Description="表单名称" />
        <Column Name="Description" DataType="String" Length="200" Description="描述" />
        <Column Name="Enable" DataType="Boolean" Description="启用" />
        <Column Name="CreateUserID" DataType="Int32" Description="创建者" Category="扩展信息" />
        <Column Name="CreateTime" DataType="DateTime" Description="创建时间" Category="扩展信息" />
        <Column Name="CreateIP" DataType="String" Description="创建地址" Category="扩展信息" />
        <Column Name="UpdateUserID" DataType="Int32" Description="更新者" Category="扩展信息" />
        <Column Name="UpdateTime" DataType="DateTime" Description="更新时间" Category="扩展信息" />
        <Column Name="UpdateIP" DataType="String" Description="更新地址" Category="扩展信息" />
      </Columns>
      <Indexes>
        <Index Columns="Name" Unique="True" />
        <Index Columns="Enable" />
      </Indexes>
    </Table>
    <Table Name="CmsFormField" Description="自定义表单字段">
      <Columns>
        <Column Name="ID" DataType="Int32" Identity="True" PrimaryKey="True" Description="编号" />
        <Column Name="FormID" DataType="Int32" Map="CmsForm@ID@Name" Description="表单编号" />
        <Column Name="Name" DataType="String" Master="True" Description="字段名" />
        <Column Name="FieldType" DataType="Int32" Description="字段类型" Type="LeoChen.Cms.Data.CmsItemType" />
        <Column Name="DisplayName" DataType="String" Description="显示名" />
        <Column Name="Length" DataType="Int32" Description="长度" />
        <Column Name="DefaultValue" DataType="String" Length="200" Description="默认值" />
        <Column Name="Description" DataType="String" Length="200" Description="描述" />
        <Column Name="Sorting" DataType="Int32" Description="排序" />
        <Column Name="Enable" DataType="Boolean" Description="启用" />
        <Column Name="CreateUserID" DataType="Int32" Description="创建者" Category="扩展信息" />
        <Column Name="CreateTime" DataType="DateTime" Description="创建时间" Category="扩展信息" />
        <Column Name="CreateIP" DataType="String" Description="创建地址" Category="扩展信息" />
        <Column Name="UpdateUserID" DataType="Int32" Description="更新者" Category="扩展信息" />
        <Column Name="UpdateTime" DataType="DateTime" Description="更新时间" Category="扩展信息" />
        <Column Name="UpdateIP" DataType="String" Description="更新地址" Category="扩展信息" />
      </Columns>
      <Indexes>
        <Index Columns="Name" Unique="True" />
        <Index Columns="FormID" />
        <Index Columns="Enable" />
      </Indexes>
    </Table>
    <Table Name="CmsExtForm" Description="自定义表单数据">
      <Columns>
        <Column Name="ID" DataType="Int32" Identity="True" PrimaryKey="True" Description="编号" />
        <Column Name="FormID" DataType="Int32" Map="CmsForm@ID@Name" Description="表单编号" />
        <Column Name="FormFieldID" DataType="Int32" Map="CmsFormField@ID@Name" Description="字段编号" />
        <Column Name="FieldValue" DataType="String" Length="600" Description="默认值" />
      </Columns>
      <Indexes>
        <Index Columns="FormFieldID,FormID" Unique="True" />
      </Indexes>
    </Table>
  </Tables>
</EntityModel>