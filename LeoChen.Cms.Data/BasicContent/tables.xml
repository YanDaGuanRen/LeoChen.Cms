<?xml version="1.0" encoding="utf-8"?>
<EntityModel xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:schemaLocation="https://newlifex.com https://newlifex.com/Model202509.xsd" schemaLocation="https://newlifex.com/Model202407.xsd" Document="https://newlifex.com/xcode/model" xmlns="https://newlifex.com/Model202509.xsd">
  <Option>
    <!--类名模板。其中{name}替换为Table.Name，如{name}Model/I{name}Dto等-->
    <ClassNameTemplate />
    <!--显示名模板。其中{displayName}替换为Table.DisplayName-->
    <DisplayNameTemplate />
    <!--基类。可能包含基类和接口，其中{name}替换为Table.Name-->
    <BaseClass>Entity</BaseClass>
    <!--命名空间-->
    <Namespace>LeoChen.Cms.Data</Namespace>
    <!--输出目录-->
    <Output>.\</Output>
    <!--是否使用中文文件名。默认false-->
    <ChineseFileName>False</ChineseFileName>
    <!--用于生成Copy函数的参数类型。例如{name}或I{name}-->
    <ModelNameForCopy />
    <!--带有索引器。实现IModel接口-->
    <HasIModel>False</HasIModel>
    <!--可为null上下文。生成String?等-->
    <Nullable>False</Nullable>
    <!--数据库连接名-->
    <ConnName>Membership</ConnName>
    <!--模型类模版。设置后生成模型类，用于接口数据传输，例如{name}Model-->
    <ModelClass>{name}Model</ModelClass>
    <!--模型类输出目录。默认当前目录的Models子目录-->
    <ModelsOutput>.\Models\</ModelsOutput>
    <!--模型接口模版。设置后生成模型接口，用于约束模型类和实体类，例如I{name}-->
    <ModelInterface>I{name}</ModelInterface>
    <!--模型接口输出目录。默认当前目录的Interfaces子目录-->
    <InterfacesOutput>.\Interfaces\</InterfacesOutput>
    <!--用户实体转为模型类的模型类。例如{name}或{name}DTO-->
    <ModelNameForToModel />
    <!--命名格式。Default/Upper/Lower/Underline-->
    <NameFormat>Default</NameFormat>
    <!--魔方区域显示名-->
    <DisplayName>基础内容</DisplayName>
    <!--魔方控制器输出目录-->
    <CubeOutput>../../LeoChen.Cms/Areas/BasicContent</CubeOutput>
  </Option>
  <Tables>
    <Table Name="CmsSite" Description="站点管理表">
      <Columns>
        <Column Name="ID" DataType="Int32" Identity="True" PrimaryKey="True" Description="主键ID" />
        <Column Name="AreaID" DataType="Int32" Map="CmsArea@ID@Name" Description="区域代码" />
        <Column Name="Title" DataType="String" Master="True" Length="100" Description="标题" />
        <Column Name="Subtitle" DataType="String" Length="200" Description="副标题" />
        <Column Name="Domain" DataType="String" Description="域名" />
        <Column Name="Logo" DataType="String" Length="100" Description="logo" />
        <Column Name="Keywords" DataType="String" Length="200" Description="关键词" />
        <Column Name="Description" DataType="String" Length="500" Description="描述" />
        <Column Name="Icp" DataType="String" Length="30" Description="icp" />
        <Column Name="Theme" DataType="String" Length="30" Description="模板名" />
        <Column Name="Statistical" DataType="String" Length="500" Description="statistical" />
        <Column Name="Copyright" DataType="String" Length="200" Description="copyright" />
        <Column Name="CreateUserID" DataType="Int32" Description="创建者" Category="扩展信息" />
        <Column Name="CreateTime" DataType="DateTime" Description="创建时间" Category="扩展信息" />
        <Column Name="CreateIP" DataType="String" Description="创建地址" Category="扩展信息" />
        <Column Name="UpdateUserID" DataType="Int32" Description="更新者" Category="扩展信息" />
        <Column Name="UpdateTime" DataType="DateTime" Description="更新时间" Category="扩展信息" />
        <Column Name="UpdateIP" DataType="String" Description="更新地址" Category="扩展信息" />
      </Columns>
      <Indexes>
        <Index Columns="AreaID" Unique="True" />
      </Indexes>
    </Table>
    <Table Name="CmsCompany" Description="公司信息表">
      <Columns>
        <Column Name="ID" DataType="Int32" Identity="True" PrimaryKey="True" Description="主键ID" />
        <Column Name="AreaID" DataType="Int32" Map="CmsArea@ID@Name" Description="站点代码" />
        <Column Name="Name" DataType="String" Master="True" Length="100" Description="名称" />
        <Column Name="Address" DataType="String" Length="200" Description="地址" />
        <Column Name="Postcode" DataType="String" Length="6" Description="邮编" />
        <Column Name="Contact" DataType="String" Length="30" Description="联系人" />
        <Column Name="Mobile" DataType="String" Length="30" Description="手机号" />
        <Column Name="Phone" DataType="String" Length="30" Description="电话" />
        <Column Name="Fax" DataType="String" Length="30" Description="传真" />
        <Column Name="Email" DataType="String" Description="邮箱" />
        <Column Name="QQ" DataType="String" Length="30" Description="QQ号" />
        <Column Name="Weixin" DataType="String" Length="30" Description="微信号" />
        <Column Name="Blicense" DataType="String" Length="20" Description="营业执照" />
        <Column Name="Other" DataType="String" Length="200" Description="其他" />
        <Column Name="CreateUserID" DataType="Int32" Description="创建者" Category="扩展信息" />
        <Column Name="CreateTime" DataType="DateTime" Description="创建时间" Category="扩展信息" />
        <Column Name="CreateIP" DataType="String" Description="创建地址" Category="扩展信息" />
        <Column Name="UpdateUserID" DataType="Int32" Description="更新者" Category="扩展信息" />
        <Column Name="UpdateTime" DataType="DateTime" Description="更新时间" Category="扩展信息" />
        <Column Name="UpdateIP" DataType="String" Description="更新地址" Category="扩展信息" />
      </Columns>
      <Indexes>
        <Index Columns="AreaID" Unique="True" />
      </Indexes>
    </Table>
    <Table Name="CmsContent_Sort" Description="内容分类表">
      <Columns>
        <Column Name="ID" DataType="Int32" Identity="True" PrimaryKey="True" Description="主键ID" />
        <Column Name="AreaID" DataType="Int32" Map="CmsArea@ID@Name" Description="区域代码" />
        <Column Name="Pid" DataType="Int32" Description="父级代码" />
        <Column Name="ModelID" DataType="Int32" Map="CmsModel@ID@Name" Description="模型代码" />
        <Column Name="Name" DataType="String" Master="True" Length="100" Description="名称" />
        <Column Name="Subname" DataType="String" Length="200" Description="副名称" />
        <Column Name="ListTpl" DataType="String" Description="列表模板" />
        <Column Name="ContentTpl" DataType="String" Description="内容模板" />
        <Column Name="Status" DataType="Boolean" Description="状态" />
        <Column Name="Outlink" DataType="String" Length="100" Description="外部链接" />
        <Column Name="Ico" DataType="String" Length="100" Description="图标" />
        <Column Name="Pic" DataType="String" Length="100" Description="图片" />
        <Column Name="Title" DataType="String" Length="100" Description="标题" />
        <Column Name="Keywords" DataType="String" Length="200" Description="关键词" />
        <Column Name="Description" DataType="String" Length="500" Description="描述" />
        <Column Name="Filename" DataType="String" Length="30" Description="文件名" />
        <Column Name="Sorting" DataType="Int32" Description="排序" />
        <Column Name="Def1" DataType="String" Length="1000" Description="预留字段1" Category="扩展信息" />
        <Column Name="Def2" DataType="String" Length="1000" Description="预留字段2" Category="扩展信息" />
        <Column Name="Def3" DataType="String" Length="1000" Description="预留字段3" Category="扩展信息" />
        <Column Name="Def4" DataType="String" Length="1000" Description="预留字段1" Category="扩展信息" />
        <Column Name="Def5" DataType="String" Length="1000" Description="预留字段2" Category="扩展信息" />
        <Column Name="Def6" DataType="String" Length="1000" Description="预留字段3" Category="扩展信息" />
        <Column Name="CreateUserID" DataType="Int32" Description="创建者" Category="扩展信息" />
        <Column Name="CreateTime" DataType="DateTime" Description="创建时间" Category="扩展信息" />
        <Column Name="CreateIP" DataType="String" Description="创建地址" Category="扩展信息" />
        <Column Name="UpdateUserID" DataType="Int32" Description="更新者" Category="扩展信息" />
        <Column Name="UpdateTime" DataType="DateTime" Description="更新时间" Category="扩展信息" />
        <Column Name="UpdateIP" DataType="String" Description="更新地址" Category="扩展信息" />
      </Columns>
      <Indexes>
        <Index Columns="AreaID" />
        <Index Columns="PID" />
        <Index Columns="AreaID,PID" />
        <Index Columns="ModelID" />
        <Index Columns="Sorting" />
        <Index Columns="Name" Unique="True" />
      </Indexes>
    </Table>
  </Tables>
</EntityModel>